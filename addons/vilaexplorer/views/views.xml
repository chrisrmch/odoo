<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="vilaexplorer.list">
      <field name="name">vilaexplorer list</field>
      <field name="model">vilaexplorer.vilaexplorer</field>
      <field name="arch" type="xml">  
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>

 
    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="vilaexplorer.action_window">
      <field name="name">vilaExplorer window</field>
      <field name="res_model">vilaexplorer.vilaexplorer</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="vilaexplorer.action_server">
      <field name="name">vilaExplorer server</field>
      <field name="model_id" ref="model_vilaexplorer_vilaexplorer"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="vilaExplorer" id="vilaexplorer.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="vilaexplorer.menu_1" parent="vilaexplorer.menu_root"/>
    <menuitem name="Menu 2" id="vilaexplorer.menu_2" parent="vilaexplorer.menu_root"/>

     <!-- actions -->  

    <menuitem name="List" id="vilaexplorer.menu_1_list" parent="vilaexplorer.menu_1" action="vilaexplorer.action_window"/>
    <menuitem name="Server to list" id="vilaexplorer" parent="vilaexplorer.menu_2" action="vilaexplorer.action_server"/>

  </data>
</odoo>